                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="5dp"
                android:paddingLeft="5dp">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingRight="5dp"
                    android:layout_weight="1">

                    <TextView
                        android:layout_width="70dp"
                        android:gravity="center"
                        android:layout_height="wrap_content"
                        android:text="@string/turnip_tue_am"
                        android:background="@drawable/item_title"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="10dp"
                        android:padding="5dp"
                        android:textColor="@color/colorPrimaryDark"
                        android:textStyle="bold"
                        android:textSize="13dp"
                        android:fontFamily="@font/cookierun_regular"/>

                    <TextView
                        android:id="@+id/tvturTUEam"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="@string/click_to_input"
                        android:textSize="13dp"
                        android:textColor="#222"
                        android:fontFamily="@font/cookierun_regular"/>
                </LinearLayout>  <!-- AM -->

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingLeft="5dp"
                    android:layout_weight="1">

                    <TextView
                        android:layout_width="70dp"
                        android:gravity="center"
                        android:layout_height="wrap_content"
                        android:text="@string/turnip_tue_pm"
                        android:background="@drawable/item_title"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="10dp"
                        android:padding="5dp"
                        android:textColor="@color/colorPrimaryDark"
                        android:textStyle="bold"
                        android:textSize="13dp"
                        android:fontFamily="@font/cookierun_regular"/>

                    <TextView
                        android:id="@+id/tvturTUEpm"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="@string/click_to_input"
                        android:textSize="13dp"
                        android:textColor="#222"
                        android:fontFamily="@font/cookierun_regular"/>
                </LinearLayout>  <!-- PM -->
            </LinearLayout>  <!-- TUE -->

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="5dp"
                android:paddingLeft="5dp">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingRight="5dp"
                    android:layout_weight="1">

                    <TextView
                        android:layout_width="70dp"
                        android:gravity="center"
                        android:layout_height="wrap_content"
                        android:text="@string/turnip_wed_am"
                        android:background="@drawable/item_title"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="10dp"
                        android:padding="5dp"
                        android:textColor="@color/colorPrimaryDark"
                        android:textStyle="bold"
                        android:textSize="13dp"
                        android:fontFamily="@font/cookierun_regular"/>

                    <TextView
                        android:id="@+id/tvturWEDam"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="@string/click_to_input"
                        android:textSize="13dp"
                        android:textColor="#222"
                        android:fontFamily="@font/cookierun_regular"/>
                </LinearLayout>  <!-- AM -->

                <LinearLayout
